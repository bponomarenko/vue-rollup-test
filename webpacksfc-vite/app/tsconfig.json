{
    "compilerOptions": {
        "baseUrl": "src",
        "esModuleInterop": true,
        "sourceMap": true,
        "typeRoots": ["node_modules/@types"],
        "paths": {
            "@/*": ["src/*"]
        },
        "isolatedModules": true,
        "noImplicitThis": true,
        "module": "commonjs",
        "target": "es6"
    },

    "vueCompilerOptions": {
        "target": 2.7
    }
}
